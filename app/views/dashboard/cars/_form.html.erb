<div class="container">
  <div class="row">
    <div class="col-xs-offest-1 col-xs-10 col-xs-offset-1 col-lg-offset-3 col-lg-6">
      <%= simple_form_for([:dashboard, @car]) do |f| %>
      <%= f.input :brand %>
      <%= f.input :model %>
      <%= f.input :color %>
      <%= f.input :description %>
      <%= f.input :photo, as: :attachinary %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
