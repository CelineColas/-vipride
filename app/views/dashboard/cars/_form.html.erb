<%= simple_form_for([:dashboard, @car]) do |f| %>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :model %>
  <%= f.text_field :model %>
  <%= f.label :color %>
  <%= f.text_field :color %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.input :photo, as: :attachinary %>
  <%= f.submit %>
<% end %>
